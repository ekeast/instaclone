<h1>Instaclone</h1>
<div class="posts-wrapper row">
  <% @posts.each do |post| %>
    <div class="post">
      <div class="post-head">
        <h3 class="name">Ted Keast</h3>
      </div>
        <%= link_to image_tag(post.image(:medium)), post_path(post) %>
        <p><%= post.caption %></p>
      </div>
  <% end %>
</div>
